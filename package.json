    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "html-webpack-plugin": "^5.5.0",
    "styled-components": "^6.0.0-rc.3",
    "webpack": "^5.72.1",
