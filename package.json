{
  "name": "create-new-startup",
  "version": "1.2.9",
  "description": "A React Framework for Startup Founders",
  "main": "./frontend/index.js",
  "bin": {
    "create-new-startup": "bin/installScript.js"
  },
  "engines": {
    "node": "17.x"
  },
  "proxy": "http://localhost:5000/",
  "scripts": {
    "start-frontend": "webpack-dev-server .",
    "start-backend": "nodemon ./backend/server.js",
    "start": "concurrently \"npx kill-port 6253 && npm run start-backend\" \"npx kill-port 5000 && npm run start-frontend\"",
    "build": "webpack --mode production",
    "heroku-prebuild": "npm install --dev",
    "heroku-postbuild": "npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "keywords": [
    "startups",
    "pivot",
    "react",
    "express",
    "mongoDB",
    "node",
    "fast",
    "new",
    "startup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibichomurage/create-new-startup.git"
  },
  "author": "Murage Kibicho",
  "license": "ISC",
